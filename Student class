package studentgroop;

	
	
	public class Student extends Human {
		
		public Student() {};

		public Student(String name, String lastname, int age, char sex, int course) {
			super(name, lastname, age, sex);
			this.course = course;
		}

		private int course;

		public int getCourse() {
			return course;
		}

		public void setCourse(int course) {
			this.course = course;
		}

		@Override
		public String printinfo() {
			return super.printinfo() + " course : = " + this.getCourse();
		}

	

	}
